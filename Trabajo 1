Num_1=int(input("Ingrese valor 1: "))
Num_2=int(input("Ingrese valor 2: "))

if (Num_2==0):
     print("Dato invalido")
     Num_2= int (input("Ingrese nuevo valor")) 
else:
    pass

R_suma =(Num_1 + Num_2)
R_resta =(Num_1 - Num_2)
R_division =(Num_1 / Num_2)
R_multiplicacion =(Num_1 * Num_2) 
print(f"El resultado de la suma es:{R_suma}" )
print(f"El resultado de la resta es:{R_resta}" )
print(f"El resultado de la division es:{R_division}" )
print(f"El resultado de la multiplicacion es:{R_multiplicacion}" )


